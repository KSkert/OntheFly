{
  "name": "onthefly",
  "displayName": "On the Fly",
  "description": "Mid-training model adjustment toolkit for PyTorch.",
  "version": "0.0.1",
  "publisher": "Luke Skertich",
  "engines": {
    "vscode": "^1.102.0"
  },
  "activationEvents": [
    "onCommand:onthefly.showDashboard"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "onthefly.showDashboard",
        "title": "On the Fly: Show Dashboard"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "On the Fly",
      "properties": {
        "onthefly.pythonPath": {
          "type": "string",
          "default": "python",
          "description": "Path to Python interpreter running OnTheFlySession"
        },
        "onthefly.storagePathOverride": {
          "type": "string",
          "default": "",
          "description": "Override path for the SQLite DB (for advanced setups)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "rebuild:electron": "electron-rebuild -f -v 35.6.0 --arch=arm64"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.0.0",
    "@types/vscode": "^1.102.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "chart.js": "^3.9.1"
  }
}
