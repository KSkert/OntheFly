{
  "name": "onthefly",
  "displayName": "OnTheFly",
  "description": "Human-in-the-Loop ML Training Orchestrator.",
  "version": "0.1.0",
  "publisher": "lukeskertich",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KSkert/OntheFly"
  },
  "homepage": "https://github.com/KSkert/OntheFly",
  "bugs": {
    "url": "https://github.com/KSkert/OntheFly/issues"
  },
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "onthefly.showDashboard",
        "title": "OnTheFly: Show Dashboard"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "On the Fly",
      "properties": {
        "onthefly.pythonPath": {
          "type": "string",
          "default": "python",
          "description": "Path to Python interpreter running OnTheFlySession (>=3.9)."
        },
        "onthefly.storagePathOverride": {
          "type": "string",
          "default": "",
          "description": "Override path for the SQLite DB (advanced)."
        },
        "onthefly.minPythonPackage": {
          "type": "string",
          "default": "0.0.5",
          "description": "Minimum onthefly-ai version required by the extension."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "rebuild:electron": "electron-rebuild -f -v 35.6.0 --arch=arm64"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.0.0",
    "@types/semver": "^7.7.1",
    "@types/vscode": "^1.102.0",
    "electron-rebuild": "^3.2.9",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "chart.js": "^3.9.1",
    "semver": "^7.7.2"
  }
}
