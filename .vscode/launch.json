{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run On the Fly Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/vscode-onthefly"
      ],
      "outFiles": [
        "${workspaceFolder}/vscode-onthefly/out/**/*.js"
      ],
      "preLaunchTask": "tsc: watch - vscode-onthefly"
    }
  ]
}
