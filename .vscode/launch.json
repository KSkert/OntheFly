{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Seamless Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/vscode-seamless"
      ],
      "outFiles": [
        "${workspaceFolder}/vscode-seamless/out/**/*.js"
      ],
      "preLaunchTask": "tsc: watch - vscode-seamless"
    }
  ]
}
